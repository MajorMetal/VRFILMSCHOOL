<main class='main'>
	<div class='container clearfix'>
		<h3 class='section-title'>Instructors</h3>
		<div class='column max-width-sml clearfix'>
			<% @speakers.each do |speaker| %>
				<div class="speaker-img-cont <%= speaker.custom_class %>">
					<img class='speaker-img' src="<%= asset_path("#{speaker.img}") %>" />
					<p class='speaker-info name'><%= speaker.name %></p>
					<p class='speaker-info'><%= speaker.bio %></p>
				</div>
			<% end %>
		</div>
	</div>
	<%= render 'partials/map' %>
	<%= render 'partials/uciMapScript' %>
</main>
